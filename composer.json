{
    "authors": [
        {
            "name": "Serge Yakovlev",
            "email": "serge.yakovlev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sergeyakovlev\\Psr20\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SergeYakovlev\\Psr20\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        },
        "sort-packages": true
    },
    "description": "Implementation of the PSR-20 Clock Interface",
    "homepage": "https://github.com/sergeyakovlev/psr-20",
    "keywords": [
        "clock",
        "clock-implementation",
        "implementation",
        "php",
        "psr-20",
        "psr20"
    ],
    "license": "MIT",
    "name": "sergeyakovlev/psr-20",
    "require": {
        "php": "^8.0",
        "psr/clock": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0",
        "psalm/plugin-phpunit": "^0.18",
        "squizlabs/php_codesniffer": "3.7",
        "vimeo/psalm": "5.9"
    },
    "scripts": {
        "analyze-phpstan": "vendor/bin/phpstan",
        "analyze-psalm": "vendor/bin/psalm",
        "cs": "vendor/bin/phpcs",
        "cs-fix": "vendor/bin/phpcbf",
        "test": "vendor/bin/phpunit"
    },
    "type": "library"
}
